<%= form_for [:admin, @update] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.label :published %><br />
    <%= f.check_box :published %>
  </p>
  <p>
    <%= f.label :data_inicio %><br />
    <%= f.datetime_select :data_inicio %>
  </p>
  <p>
    <%= f.label :data_fim %><br />
    <%= f.datetime_select :data_fim %>
  </p>
  <p><%= f.submit %></p>
<% end %>
